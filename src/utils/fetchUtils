export async function weatherApi(location) {
    const res = await fetch(`https://api.openweathermap.org/data/2.5/weather?q=${location}&APPID=6f3fb3be216b6504d1b69314090abcd6&units=metric`).catch((err)=> {
        console.log(err)
    })
await new Promise((resolve) => setTimeout(resolve,1000))
    return res.json()
}